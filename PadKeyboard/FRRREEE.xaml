<Window x:Class="PadKeyboard.FRRREEE"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PadKeyboard"
        mc:Ignorable="d"
        WindowState="Maximized"
        Top="0"
        Left="0"
        Background="#77777777"
        WindowStyle="None"
        Title="Soakupthesun">
    <Window.Resources>
        <ResourceDictionary Source="Css.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Grid Name="keysPanel" />
        <Grid>
            <Ellipse Name="settingsOutter" StrokeThickness="0.5" Opacity="0.1"/>
            <Ellipse Name="settings" Width="55" Height="55"/>
            <TextBlock Name="count_max" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#fff" />
        </Grid>
    </Grid>
</Window>
