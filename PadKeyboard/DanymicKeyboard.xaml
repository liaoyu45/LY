<Window x:Class="PadKeyboard.DanymicKeyboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PadKeyboard"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowState="Maximized"
        WindowStyle="None"
        Top="0"
        Left="0"
        BorderThickness="0"
        Background="#11000000"
        AllowsTransparency="True"
        Title="DanymicKeyboard">
    <Window.Resources>
        <ResourceDictionary Source="Css.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid Name="path" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Slider Name="weight" Value="88" Maximum="99" Minimum="66" HorizontalAlignment="Center" Width="144" VerticalAlignment="Top" Margin="0,44,0,0"></Slider>
    </Grid>
</Window>
